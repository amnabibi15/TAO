   "source": [
    "casas_data_dir = f'{global_datadir}/casas-dataset'\n",
    "home_count = 30\n",
    "home_prefix = 'csh1'\n",
    "data_columns = [\"DateTime\",\"Sensor\",\"Location\",\"Location_Specific\",\"Message\",\"SensorType\",\"Activity\"]\n",
    "\n",
    "df3_all = None\n",
    "\n",
    "for home_id in range(1,home_count+1):\n",
    "    home_name = f'{home_prefix}{str(home_id).zfill(2)}'\n",
    "    df3_annotated = pd.read_csv(f\"{casas_data_dir}/{home_name}/{home_name}.ann.txt\",delimiter=\"\\t\",names=data_columns)\n",
    "    df3_annotated['Home'] = home_name\n",
    "    df3_annotated = df3_annotated[['Home','DateTime','Location','Location_Specific','Activity']]\n",
    "    df3_annotated.info()\n",
    "    if df3_all is None:\n",
    "        df3_all = df3_annotated.copy(deep=True)\n",
    "    else:\n",
    "        df3_all = pd.concat([df3_all, df3_annotated],ignore_index=True)\n",
    "    print(f\"Current Home, {home_name}, df size, {df3_all.shape}\")\n"
   ]
