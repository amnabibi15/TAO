    "df6_all = None\n",
    "default_start_time = datetime.now()\n",
    "FPS = 25\n",
    "df6_context_labels = []\n",
    "for uuid_dir in glob.glob(f'{tsu_data_dir}/*'):\n",
    "    for session_file in glob.glob(f\"{uuid_dir}/*.csv\"):\n",
    "        session_id = session_file.split(\"/\")[-1].split(\".\")[0]\n",
    "        df6_session = pd.read_csv(session_file)\n",
    "        total_session_seconds = df6_session['end_frame'].max() / FPS\n",
    "        df6_session['start_time'] = df6_session['start_frame'].apply(lambda x: default_start_time + timedelta(seconds=x//FPS, milliseconds=1000*(x%FPS)/FPS))\n",
    "        df6_session['end_time'] = df6_session['end_frame'].apply(lambda x: default_start_time + timedelta(seconds=x//FPS, milliseconds=1000*(x%FPS)/FPS))\n",
    "        df6_session['start_timestamp'] = df6_session['start_time'].astype(int) //10**9\n",
    "        df6_session['end_timestamp'] = df6_session['end_time'].astype(int) //10**9\n",
    "        df6_session_new = pd.DataFrame(data = range(df6_session['start_timestamp'].min(), df6_session['end_timestamp'].max()), columns=['timestamp'])\n",
    "        df6_session_new['session_id'] = session_id\n",
    "        df6_session_new['activity'] = ''\n",
    "        df6_session_new = df6_session_new.set_index('timestamp')\n",
    "        for row_idx, row in df6_session.iterrows():\n",
    "            if row['event'] in activity_map.keys():\n",
    "                df6_session_new.loc[row['start_timestamp']:row['end_timestamp'],'activity'] = \\\n",
    "                    df6_session_new.loc[row['start_timestamp']:row['end_timestamp'],'activity'].apply(\n",
    "                    lambda x: (str(x)+\",\"+str(activity_map[row['event']])))\n",
    "            else:\n",
    "                df6_context_labels.append((session_id,row['start_timestamp'], row['end_timestamp'],row['event']))\n",
    "                \n",
    "        df6_session_new = df6_session_new.reset_index()[['session_id','timestamp','activity']]\n",
    "        if df6_all is None:\n",
    "            df6_all = df6_session_new.copy(deep=True)\n",
    "        else:\n",
    "            df6_all = pd.concat([df6_all, df6_session_new],ignore_index=True)\n",
    "        print(f\"Completed Session {session_file}\")\n",
    "            "
