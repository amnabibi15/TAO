    "    df4_labels = pd.DataFrame(Y, columns=label_names)\n",
    "    df4_labels['timestamp'] = timestamps\n",
    "    df4_labels['uuid'] = uuid\n",
    "    df4_labels = df4_labels[['uuid','timestamp']+label_names]\n",
    "    df4_missing = pd.DataFrame(M, columns=label_names)\n",
    "    df4_missing['timestamp'] = timestamps\n",
    "    df4_missing['uuid'] = uuid\n",
    "    df4_missing = df4_missing[['uuid','timestamp']+label_names]\n",
    "    df4_uuid = pd.merge(df4_labels, df4_missing, on='timestamp',suffixes=('','_missing'))\n",
