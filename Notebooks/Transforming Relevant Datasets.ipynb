    "dataset4_locations = []\n",
    "obj = TimezoneFinder()\n",
    "list_uuid_files = glob.glob(f\"{extrasensory_loc_data_dir}/*\")\n",
    "list_uuids = [uuid_file.split(\"/\")[-1].split(\".\")[0] for uuid_file in list_uuid_files]\n",
    "len(list_uuids), list_uuids[:4]\n",
    "for uuid in list_uuids:\n",
    "    user_data_file = f'{extrasensory_loc_data_dir}/{uuid}.absolute_locations.csv.gz';\n",
    "    with gzip.open(user_data_file,'rb') as fid:\n",
    "        csv_str = fid.read().decode('utf-8');\n",
    "    csv_arr = csv_str.split(\"\\n\")\n",
    "    csv_arr = [xr.split(\",\") for xr in csv_arr[1:]]\n",
    "    df_time_loc = pd.DataFrame(csv_arr,columns=['timestamp','lat','long'])\n",
    "    user_lat, user_long = np.nanmean(df_time_loc['lat'].astype(float, errors='ignore')), np.nanmean(df_time_loc['long'].astype(float, errors='ignore'))\n",
    "    try:\n",
    "        user_timezone = obj.timezone_at(lng=user_long, lat=user_lat)\n",
    "    except:\n",
    "        print(\"ERROR\",user_lat, user_long)\n",
    "        user_timezone = 'America/Los_Angeles'\n",
    "    print(user_lat, user_long,user_timezone)\n",
    "    dataset4_locations.append([uuid, user_timezone])\n",
    "\n",
    "df4_locations = pd.DataFrame(dataset4_locations, columns=['uuid','timezone'])    \n",
    "df4_locations\n",
    "    "
   ]
