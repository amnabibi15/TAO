    "    df5_sub = df5_sub[['user','timestamp_in_millisec','locomotion','activity','left_arm','left_arm_object','right_arm','right_arm_object','both_arms']]\n",
    "    df5_sub = pd.melt(df5_sub,id_vars = ['user','timestamp_in_millisec'],var_name='observation',value_name='label')\n",
